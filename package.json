        "mini-css-extract-plugin": "^2.6.1",
        "path": "^0.12.7",
        "prettier": "^2.7.1",
        "qunit": "2.19.1",
        "react-query": "^3.39.1",
        "sass": "^1.53.0",
        "styled-components": "^5.3.5",
        "terser-webpack-plugin": "^5.3.3",
        "webpack": "^5.73.0"
    },
    "dependencies": {
        "@lodder/grunt-postcss": "^3.1.1",
