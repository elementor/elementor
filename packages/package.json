{
  "name": "elementor-packages",
  "author": "Elementor Team",
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.0.0",
  "scripts": {
    "build": "turbo build",
    "build:tools": "turbo build --filter=\"./packages/tools/*\"",
    "dev": "concurrently -n turbo,tsc -c red,blue 'turbo dev --parallel' 'tsc -w --preserveWatchOutput'",
    "test": "jest --config='./jest.config.js'",
    "test:coverage": "npm run test -- --coverage",
    "lint": "concurrently -n eslint,tsc -c magenta,blue \"eslint . --report-unused-disable-directives-severity error\" \"tsc\"",
    "check-unused": "knip",
    "spell-check": "cspell . --quiet",
    "format": "eslint . --fix",
    "changelog": "changeset",
    "release": "npm run build && changeset publish",
    "docs": "typedoc --options ./typedoc/typedoc.config.js"
  },
  "workspaces": [
    "packages/core/*",
    "packages/libs/*",
    "packages/tools/*"
  ],
  "devDependencies": {
    "@swc/core": "^1.10.1",
    "@swc/jest": "^0.2.37",
    "@tanstack/eslint-plugin-query": "^5.56.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@wordpress/eslint-plugin": "^21.1.0",
    "@wordpress/jest-console": "^8.13.0",
    "concurrently": "^9.1.0",
    "cspell": "^8.17.1",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-react-compiler": "latest",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^7.2.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.54.1",
    "msw": "^2.6.8",
    "prettier": "npm:wp-prettier@^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "typedoc": "^0.27.4",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "nwsapi": "2.2.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
