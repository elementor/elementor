{
  "name": "elementor-packages",
  "author": "Elementor's Team",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "webpack --config=webpack.prod.js",
    "dev": "concurrently -n webpack,tsc -c blue,green \"webpack -w --config webpack.dev.js\" \"tsc --watch\"",
    "test": "jest --config='./jest.config.js'",
    "test:coverage": "npm run test -- --coverage",
    "lint": "concurrently -n eslint,tsc -c magenta,green \"eslint .\" \"tsc\""
  },
  "workspaces": [
    "./packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@wordpress/eslint-plugin": "^13.7.0",
    "@wordpress/jest-console": "^6.8.0",
    "@wordpress/readable-js-assets-webpack-plugin": "^2.7.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@elementor/ui": "^1.4.42",
    "@wordpress/i18n": "^4.24.0"
  }
}
