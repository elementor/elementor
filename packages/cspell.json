{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "words": [
    "ABSPATH",
    "customfonts",
    "elementor",
    "googlefonts",
    "minifiers",
    "noflip",
    "styledef",
    "totalpages",
    "transpiling",
    "Turborepo",
    "unapply",
    "undoable",
    "vmax",
    "vmin",
    "plupload",
    "syncpack",
    "TSES",
    "overscan",
    "twing",
    "infotip",
    "notistack",
    "stylesheet",
    "flexbox",
  ],
  "enableGlobDot": true,
  "ignorePaths": [
    ".git",
    ".github",
    ".idea",
    ".vscode",
    ".cspell",
    ".turbo",
    ".DS_Store",
    "docs",
    "coverage",
    "**/dist/**",
    "**/CHANGELOG.md",
    "**/node_modules/**",
    "./apps/demo/mockServiceWorker.js"
  ],
  "ignoreRegExpList": [
	  "/from\\s+(['\"]).*\\1/"
  ],
  "cache": {
    "useCache": true,
    "cacheLocation": "./.cspell/.cspellcache",
    "cacheStrategy": "content"
  }
}
