{
  "name": "@elementor/commitlint-plugin-editor",
  "description": "Internal Commitlint plugin",
  "version": "0.0.0",
  "private": true,
  "author": "Elementor Team",
  "homepage": "https://elementor.com/",
  "license": "GPL-3.0-or-later",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementor/elementor-packages.git",
    "directory": "packages/tools/commitlint-plugin-editor"
  },
  "bugs": {
    "url": "https://github.com/elementor/elementor-packages/issues"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "/dist",
    "/src",
    "!**/__tests__"
  ],
  "peerDependencies": {
    "@commitlint/lint": "^19.8.1"
  },
  "devDependencies": {
    "@commitlint/types": "^19.8.1"
  }
}
