{
  "name": "@elementor/eslint-plugin-editor",
  "description": "Internal ESLint plugin",
  "version": "0.0.0",
  "private": true,
  "author": "Elementor Team",
  "homepage": "https://elementor.com/",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementor/elementor-packages.git",
    "directory": "packages/tools/eslint-plugin-editor"
  },
  "bugs": {
    "url": "https://github.com/elementor/elementor-packages/issues"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "/dist",
    "/src",
    "!**/__tests__"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/rule-tester": "^8.6.0",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^8.57.1",
    "typescript": "^5.7.2"
  }
}
