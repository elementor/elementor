{
  "name": "@elementor/http-client",
  "description": "Provides a simple way to make HTTP requests",
  "version": "3.32.0",
  "private": false,
  "author": "Elementor Team",
  "homepage": "https://elementor.com/",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementor/elementor.git",
    "directory": "packages/libs/http-client"
  },
  "bugs": {
    "url": "https://github.com/elementor/elementor/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "/dist",
    "/src",
    "!**/__tests__"
  ],
  "scripts": {
    "build": "tsup --config=../../tsup.build.ts",
    "dev": "tsup --config=../../tsup.dev.ts"
  },
  "dependencies": {
    "@elementor/env": "3.32.0",
    "axios": "^1.9.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5"
  }
}
