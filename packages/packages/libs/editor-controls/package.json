{
  "name": "@elementor/editor-controls",
  "description": "This package contains the controls model and utils for the Elementor editor",
  "version": "1.4.0",
  "private": false,
  "author": "Elementor Team",
  "homepage": "https://elementor.com/",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementor/elementor-packages.git",
    "directory": "packages/libs/editor-controls"
  },
  "bugs": {
    "url": "https://github.com/elementor/elementor-packages/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "/dist",
    "/src",
    "!**/__tests__"
  ],
  "scripts": {
    "build": "tsup --config=../../tsup.build.ts",
    "dev": "tsup --config=../../tsup.dev.ts"
  },
  "dependencies": {
    "@elementor/editor-current-user": "0.6.0",
    "@elementor/editor-elements": "0.9.1",
    "@elementor/editor-props": "0.17.0",
    "@elementor/editor-responsive": "0.13.6",
    "@elementor/editor-ui": "0.14.1",
    "@elementor/editor-v1-adapters": "0.12.1",
    "@elementor/env": "0.3.5",
    "@elementor/http-client": "0.3.0",
    "@elementor/icons": "^1.47.0",
    "@elementor/locations": "0.8.0",
    "@elementor/query": "0.2.4",
    "@elementor/session": "0.1.0",
    "@elementor/ui": "1.36.2",
    "@elementor/utils": "0.5.0",
    "@elementor/wp-media": "0.6.1",
    "@wordpress/i18n": "^5.13.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
