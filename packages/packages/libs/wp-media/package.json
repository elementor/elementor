{
  "name": "@elementor/wp-media",
  "description": "An adapter for WordPress' media utils",
  "version": "0.6.1",
  "private": false,
  "author": "Elementor Team",
  "homepage": "https://elementor.com/",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementor/elementor.git",
    "directory": "packages/libs/wp-media"
  },
  "bugs": {
    "url": "https://github.com/elementor/elementor/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "/dist",
    "/src",
    "!**/__tests__"
  ],
  "scripts": {
    "build": "tsup --config=../../tsup.build.ts",
    "dev": "tsup --config=../../tsup.dev.ts"
  },
  "dependencies": {
    "@elementor/query": "0.2.4",
    "@elementor/utils": "0.5.0"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "tsup": "^8.3.5"
  }
}
