{
  "name": "@elementor/editor-app-bar",
  "description": "App Bar extension for @elementor/editor",
  "version": "0.32.0",
  "private": false,
  "author": "Elementor Team",
  "homepage": "https://elementor.com/",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementor/elementor-packages.git",
    "directory": "packages/core/editor-app-bar"
  },
  "bugs": {
    "url": "https://github.com/elementor/elementor-packages/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "/dist",
    "/src",
    "!**/__tests__"
  ],
  "scripts": {
    "build": "tsup --config=../../tsup.build.ts",
    "dev": "tsup --config=../../tsup.dev.ts"
  },
  "dependencies": {
    "@elementor/editor": "0.21.0",
    "@elementor/editor-documents": "0.13.9",
    "@elementor/editor-responsive": "0.13.6",
    "@elementor/editor-v1-adapters": "0.12.1",
    "@elementor/icons": "1.46.0",
    "@elementor/locations": "0.8.0",
    "@elementor/menus": "0.1.5",
    "@elementor/ui": "1.36.2",
    "@wordpress/i18n": "^5.13.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
